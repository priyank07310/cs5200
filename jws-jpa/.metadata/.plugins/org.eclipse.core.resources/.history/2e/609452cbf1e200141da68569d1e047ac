package edu.neu.cs5200.assign.orm.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class Tower{

	@Id
	private Integer tid;
	private String tname;
	private Integer height;
	private Integer sides;
	private Integer siteid;
	
	
	public Tower(Integer tid, String tname, Integer height, Integer sides, Integer siteid) {
		super();
		this.tid = tid;
		this.tname = tname;
		this.height = height;
		this.sides = sides;
		this.siteid = siteid;
	}


	public Tower() {
		super();
	}


	public int getTid() {
		return tid;
	}


	public void setTid(Integer tid) {
		this.tid = tid;
	}


	public String getTname() {
		return tname;
	}


	public void setTname(String tname) {
		this.tname = tname;
	}


	public int getHeight() {
		return height;
	}


	public void setHeight(Integer height) {
		this.height = height;
	}


	public int getSides() {
		return sides;
	}


	public void setSides(Integer sides) {
		this.sides = sides;
	}


	public int getId() {
		return siteid;
	}


	public void setId(Integer siteid) {
		this.siteid = siteid;
	}
	
	
}
